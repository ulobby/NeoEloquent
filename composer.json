{
    "name": "ulobby/neoeloquent",
    "description": "Laravel wrapper for the Neo4j graph database REST interface",
    "keywords": [
        "laravel",
        "neo4j",
        "graph",
        "database",
        "neoeloquent"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Abed Halawi",
            "email": "abed.halawi@vinelab.com"
        },
        {
            "name": "Ulobby",
            "email": "ulobby@ulobby.eu"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/database": "^9.0|^10.0",
        "illuminate/pagination": "^9.0|^10.0",
        "illuminate/events": "^9.0|^10.0",
        "illuminate/console": "^9.0|^10.0",
        "nesbot/carbon": "^2.66",
        "laudis/neo4j-php-client": "^2.4",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3",
        "mockery/mockery": "^1.4.2",
        "symfony/var-dumper": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "repositories": [
    ],
    "autoload": {
        "psr-4": {
            "Vinelab\\NeoEloquent\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "replace": {
        "vinelab/neoeloquent":"1.4.7"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Vinelab\\NeoEloquent\\NeoEloquentServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
